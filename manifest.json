{
	"manifest_version": 2,

	"name": "Simple Reddit",
	"description": "Reddit clutterless.",
	"version": "1.0",

	"content_scripts": [
		{
			"matches": ["*://*.reddit.com/*"],
			"js": ["js/jquery.js", "js/main.js"],
			"css": ["css/main.css"]
		}
	],
	"web_accessible_resources": [
		"img/favicon.ico",
		"img/arrows/arrow_*.png",
		"img/buttons/buttons.png"
	]
}